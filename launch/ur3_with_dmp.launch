<?xml version="1.0"?>
<launch>
    
    <include file="$(find ur_robot_driver)/launch/ur3_bringup.launch" >
      <param name="robot_ip"  default="192.168.1.128" doc="IP address by which the robot can be reached."/>
      <param name="kinematics_config"  default="${HOME}/my_robot_calibration.yaml" doc="Kinematics config file used for calibration correction. This will be used to verify the robot's calibration is matching the robot_description."/>
    </include>
    <include file="$(find dmp)/launch/dmp.launch" />
</launch>